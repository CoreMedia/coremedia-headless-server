import "templates/field.stg"


interface(type) ::= <<
!InterfaceType
name: <type.name>
<if(type.parent)>
parents:
  - <type.parent.name>
<endif>
<options(type.options)>
<if(type.fieldDefinitions)>
fields:
<type.fieldDefinitions:field()>
<endif>
>>


options(options) ::=<<
<if(options)>
options:
  <options.keys:{key | <key>: <options.(key)>};separator="\n"><endif>
>>
